            </column>
            <column name="num3" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="num4" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="num5" type="int">
                <constraints nullable="false"/>
            </column>
            <column name="date_tirage" type="date">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>